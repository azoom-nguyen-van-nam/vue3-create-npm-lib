{
  "name": "cris-input",
  "private": false,
  "version": "0.0.6",
  "type": "module",
  "files": [
    "dist",
    "src/style.css"
  ],
  "main": "./dist/cris.umd.js",
  "module": "./dist/cris.es.js",
  "exports": {
    ".": {
      "import": "./dist/cris.es.js",
      "require": "./dist/cris.umd.js"
    },
    "./dist/cris.css": {
      "import": "./dist/cris.css",
      "require": "./dist/cris.css"
    },
    "./style.css": {
      "import": "./src/style.css",
      "require": "./src/style.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "start": "vite start",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "push": "npm publish"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vue-tsc": "^1.0.11"
  }
}